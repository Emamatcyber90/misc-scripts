{
  "name": "Godmode",
  "version": "1",
  "manifest_version": 2,
  "description": "Godmode",
  "browser_action": {
    "default_icon": "img/icon128.png",
    "popup": "background.htm"
  },

   "permissions": [
       "tabs",
       "webRequest",
       "webRequestBlocking",
       "webNavigation",
       "management",
       "http://*/*",
       "https://*/*"
   ],
  "background": {
      "scripts": ["js/options.js"]
   }
}